FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
	type		movingWallVelocity;
	value		uniform (0 0 0);
    }

    right
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    top
    {
	type		slip;
    }
    
    bottom
    {
	type		fixedValue;
	value		uniform (0 0 0);
    }
    
    frontAndBack
    {
	type		empty;
    }
}

// ************************************************************************* //
